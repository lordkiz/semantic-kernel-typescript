{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "emitDecoratorMetadata": true,
    "outFile": "index.d.ts",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "es2019",
      "es2020.bigint",
      "es2020.date",
      "es2020.number",
      "es2020.promise",
      "es2020.string",
      "es2020.symbol.wellknown",
      "es2021.promise",
      "es2021.string",
      "es2021.weakref",
      "es2022.array",
      "es2022.object",
      "es2022.string",
      "es2022.error"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "types": ["reflect-metadata", "node", "jest"]
  },
  "references": [
    {
      "path": "./core"
    },
    {
      "path": "./aiservices/gemini"
    },
    {
      "path": "./aiservices/huggingface"
    },
    {
      "path": "./aiservices/openai"
    },
    {
      "path": "./agents"
    }
  ]
}
